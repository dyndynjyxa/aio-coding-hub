{
  "name": "aio-coding-hub",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc -p tsconfig.json",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "check:no-instant-now-sub": "node scripts/check-no-instant-now-sub.mjs",
    "check:precommit": "pnpm format:check && pnpm typecheck && pnpm check:no-instant-now-sub && pnpm tauri:fmt && pnpm tauri:check && pnpm tauri:clippy",
    "check:prepush": "pnpm tauri:test",
    "hooks:install": "node scripts/install-git-hooks.mjs",
    "postinstall": "node scripts/install-git-hooks.mjs",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "node scripts/tauri-dev.mjs",
    "tauri:fmt": "cd src-tauri && cargo fmt -- --check",
    "tauri:check": "cd src-tauri && cargo check --locked",
    "tauri:clippy": "cd src-tauri && cargo clippy --all-targets --locked -- -D warnings",
    "tauri:test": "cd src-tauri && cargo test --locked",
    "tauri:build": "tauri build",
    "tauri:build:mac:arm64": "tauri build --target aarch64-apple-darwin",
    "tauri:build:mac:x64": "tauri build --target x86_64-apple-darwin",
    "tauri:build:mac:universal": "tauri build --target universal-apple-darwin"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-notification": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "echarts": "^6.0.0",
    "lucide-react": "^0.562.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.11.0",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
